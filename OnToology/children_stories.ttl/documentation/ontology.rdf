    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Story -->


    <owl:Class rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Story">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
