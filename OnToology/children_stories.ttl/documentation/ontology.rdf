<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#"
     xml:base="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:ontology="http://dbpedia.org/ontology/"
     xmlns:property="http://dbpedia.org/property/"
     xmlns:children_stories="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories">
        <rdfs:comment xml:lang="en">This is an ontology about children stories</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://dbpedia.org/ontology/author -->


    <owl:ObjectProperty rdf:about="http://dbpedia.org/ontology/author">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://dbpedia.org/property/notableWorks"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Story"/>
        <rdfs:range rdf:resource="https://dbpedia.org/resource/Author"/>
        <rdfs:comment>Story is Written by Author</rdfs:comment>
        <rdfs:label xml:lang="en">hasAuthor</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://dbpedia.org/ontology/creator -->


    <owl:ObjectProperty rdf:about="http://dbpedia.org/ontology/creator">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isCreatedBy"/>
        <rdfs:domain rdf:resource="https://dbpedia.org/resource/Author"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Character"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://dbpedia.org/property/notableWorks"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#hasCharacter"/>
        </owl:propertyChainAxiom>
        <rdfs:comment>Author creates Character
If Author writes Story and Story hasCharacter, then Author creates Characters</rdfs:comment>
        <rdfs:label>createsCharacter</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://dbpedia.org/ontology/genre -->


    <owl:ObjectProperty rdf:about="http://dbpedia.org/ontology/genre">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="https://dbpedia.org/resource/Author"/>
        <rdfs:range rdf:resource="http://dbpedia.org/class/yago/WikicatLiteraryGenres"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://dbpedia.org/property/notableWorks"/>
            <rdf:Description rdf:about="http://dbpedia.org/property/genre"/>
        </owl:propertyChainAxiom>
        <rdfs:comment>Author creates work of Genre type</rdfs:comment>
        <rdfs:label>writesStoryOfGenre</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://dbpedia.org/ontology/type -->


    <owl:ObjectProperty rdf:about="http://dbpedia.org/ontology/type">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isFormatOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Story"/>
        <rdfs:range rdf:resource="http://dbpedia.org/resource/Category:Media_formats"/>
        <rdfs:comment>Story is published under Format</rdfs:comment>
        <rdfs:label>existsIn</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://dbpedia.org/property/genre -->


    <owl:ObjectProperty rdf:about="http://dbpedia.org/property/genre">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isGenreOfStory"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Story"/>
        <rdfs:range rdf:resource="http://dbpedia.org/class/yago/WikicatLiteraryGenres"/>
        <rdfs:comment>Story has Genre (e.g. Tragedy, Fantasy...)</rdfs:comment>
        <rdfs:label>belongsToGenre</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://dbpedia.org/property/notableWorks -->


    <owl:ObjectProperty rdf:about="http://dbpedia.org/property/notableWorks">
        <rdfs:domain rdf:resource="https://dbpedia.org/resource/Author"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Story"/>
        <rdfs:comment>Author writes Story</rdfs:comment>
        <rdfs:label>isAuthorOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#consumes -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#consumes">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isConsumedBy"/>
        <rdfs:domain rdf:resource="http://dbpedia.org/resource/Audience"/>
        <rdfs:range rdf:resource="http://dbpedia.org/resource/Category:Media_formats"/>
        <rdfs:comment>Audience consumes Format</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#existsInAudioFormat -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#existsInAudioFormat">
        <rdfs:subPropertyOf rdf:resource="http://dbpedia.org/ontology/type"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isAudioFormatOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Story"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Audio"/>
        <rdfs:comment>Story can be listened to</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#existsInTextFormat -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#existsInTextFormat">
        <rdfs:subPropertyOf rdf:resource="http://dbpedia.org/ontology/type"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isTextFormatOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Story"/>
        <rdfs:range rdf:resource="http://dbpedia.org/resource/Book"/>
        <rdfs:comment>Story can be read</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#existsInVideoFormat -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#existsInVideoFormat">
        <rdfs:subPropertyOf rdf:resource="http://dbpedia.org/ontology/type"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isVideoFormatOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Story"/>
        <rdfs:range rdf:resource="http://dbpedia.org/resource/Video"/>
        <rdfs:comment>Story can be watched</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#hasCharacter -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#hasCharacter">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isCharacterOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Story"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Character"/>
        <rdfs:comment>Story has Character</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#hasHero -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#hasHero">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#hasCharacter"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isHeroOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Story"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#MainCharacter"/>
        <rdfs:comment>Story has MainCharacter, which is Character</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isAudioFormatOf -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isAudioFormatOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isFormatOf"/>
        <rdfs:comment>AudioFormat is the representation of story</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isCharacterOf -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isCharacterOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Character"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Story"/>
        <rdfs:comment>Character is a character of Story</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isConsumedBy -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isConsumedBy">
        <rdfs:domain rdf:resource="http://dbpedia.org/resource/Category:Media_formats"/>
        <rdfs:range rdf:resource="http://dbpedia.org/resource/Audience"/>
        <rdfs:comment>a Media Format is consumed by an audience</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isCreatedBy -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isCreatedBy">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Character"/>
        <rdfs:range rdf:resource="https://dbpedia.org/resource/Author"/>
        <rdfs:comment>Character is created by Author
inverse property of CreatesCharacter which implies:
if Character is present in a story and story is written by Author then Character is created by Author</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isCurrentStatus -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isCurrentStatus">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isStatus"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Story"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Ongoing"/>
        <owl:propertyDisjointWith rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isEnded"/>
        <rdfs:comment>Story is not Finished</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isEnded -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isEnded">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isStatus"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Story"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Finished"/>
        <rdfs:comment>Story is Finished</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isFormatOf -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isFormatOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://dbpedia.org/resource/Category:Media_formats"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Story"/>
        <rdfs:comment>Format is the representation of Story</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isGenreOfStory -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isGenreOfStory">
        <rdfs:domain rdf:resource="http://dbpedia.org/class/yago/WikicatLiteraryGenres"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Story"/>
        <rdfs:comment>Genre is a theme of Story</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isHeroOf -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isHeroOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isCharacterOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#MainCharacter"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Story"/>
        <rdfs:comment>A MainCharacter is a Character of Story</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isInterestedByChildStory -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isInterestedByChildStory">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isInterestedByStory"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#targetsChildAudience"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#ChildAudience"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#ChildStory"/>
        <rdfs:comment>ChildAudience is interested by ChildStory</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isInterestedByStory -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isInterestedByStory">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#targetsAudience"/>
        <rdfs:domain rdf:resource="http://dbpedia.org/resource/Audience"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Story"/>
        <rdfs:comment>Audience is interested by Story</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isInterestedByTeenStory -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isInterestedByTeenStory">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isInterestedByStory"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#targetsTeenAudience"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#TeenAudience"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#TeenStory"/>
        <rdfs:comment>TeenAudience is interested by Teen Story</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isListenedToBy -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isListenedToBy">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isConsumedBy"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#listensTo"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Audio"/>
        <rdfs:range rdf:resource="http://dbpedia.org/resource/Audience"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isAudioFormatOf"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#targetsAudience"/>
        </owl:propertyChainAxiom>
        <rdfs:comment>an audio format is listen to by an audience</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isReadBy -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isReadBy">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isConsumedBy"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#reads"/>
        <rdfs:domain rdf:resource="http://dbpedia.org/resource/Book"/>
        <rdfs:range rdf:resource="http://dbpedia.org/resource/Audience"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isTextFormatOf"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#targetsAudience"/>
        </owl:propertyChainAxiom>
        <rdfs:comment>a text format is read by an audience</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isStatus -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isStatus">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Story"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Status"/>
        <rdfs:comment>Story has Status</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isTextFormatOf -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isTextFormatOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isFormatOf"/>
        <rdfs:comment>TextFormat is the representation of Story</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isVideoFormatOf -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isVideoFormatOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isFormatOf"/>
        <rdfs:comment>VideoFormat is the representation of Story</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isWatchedBy -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isWatchedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isConsumedBy"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#watches"/>
        <rdfs:domain rdf:resource="http://dbpedia.org/resource/Video"/>
        <rdfs:range rdf:resource="http://dbpedia.org/resource/Audience"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isVideoFormatOf"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#targetsAudience"/>
        </owl:propertyChainAxiom>
        <rdfs:comment>a video format is watched by an audience</rdfs:comment>
    </owl:ObjectProperty>
    


