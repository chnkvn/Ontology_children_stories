@prefix : <http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#> .

<http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories> rdf:type owl:Ontology ;
                                                                                rdfs:comment "This is an ontology about children stories"@en .

#################################################################
#    Object Properties
#################################################################

###  http://dbpedia.org/ontology/author
<http://dbpedia.org/ontology/author> rdf:type owl:ObjectProperty ;
                                     rdfs:subPropertyOf owl:topObjectProperty ;
                                     owl:inverseOf <http://dbpedia.org/property/notableWorks> ;
                                     rdfs:domain :Story ;
                                     rdfs:range <https://dbpedia.org/resource/Author> ;
                                     rdfs:comment "Story is Written by Author" ;
                                     rdfs:label "hasAuthor"@en .


###  http://dbpedia.org/ontology/creator
<http://dbpedia.org/ontology/creator> rdf:type owl:ObjectProperty ;
                                      owl:inverseOf :isCreatedBy ;
                                      rdfs:domain <https://dbpedia.org/resource/Author> ;
                                      rdfs:range :Character ;
                                      owl:propertyChainAxiom ( <http://dbpedia.org/property/notableWorks>
                                                               :hasCharacter
                                                             ) ;
                                      rdfs:comment """Author creates Character
If Author writes Story and Story hasCharacter, then Author creates Characters""" ;
                                      rdfs:label "createsCharacter" .


###  http://dbpedia.org/ontology/genre
<http://dbpedia.org/ontology/genre> rdf:type owl:ObjectProperty ;
                                    rdfs:subPropertyOf owl:topObjectProperty ;
                                    rdfs:domain <https://dbpedia.org/resource/Author> ;
                                    rdfs:range <http://dbpedia.org/class/yago/WikicatLiteraryGenres> ;
                                    owl:propertyChainAxiom ( <http://dbpedia.org/property/notableWorks>
                                                             <http://dbpedia.org/property/genre>
                                                           ) ;
                                    rdfs:comment "Author creates work of Genre type" ;
                                    rdfs:label "writesStoryOfGenre" .


###  http://dbpedia.org/ontology/type
<http://dbpedia.org/ontology/type> rdf:type owl:ObjectProperty ;
                                   owl:inverseOf :isFormatOf ;
                                   rdfs:domain :Story ;
                                   rdfs:range <http://dbpedia.org/resource/Category:Media_formats> ;
                                   rdfs:comment "Story is published under Format" ;
                                   rdfs:label "existsIn" .


###  http://dbpedia.org/property/genre
<http://dbpedia.org/property/genre> rdf:type owl:ObjectProperty ;
                                    owl:inverseOf :isGenreOfStory ;
                                    rdfs:domain :Story ;
                                    rdfs:range <http://dbpedia.org/class/yago/WikicatLiteraryGenres> ;
                                    rdfs:comment "Story has Genre (e.g. Tragedy, Fantasy...)" ;
                                    rdfs:label "belongsToGenre" .


###  http://dbpedia.org/property/notableWorks
<http://dbpedia.org/property/notableWorks> rdf:type owl:ObjectProperty ;
                                           rdfs:domain <https://dbpedia.org/resource/Author> ;
                                           rdfs:range :Story ;
                                           rdfs:comment "Author writes Story" ;
                                           rdfs:label "isAuthorOf" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#consumes
:consumes rdf:type owl:ObjectProperty ;
          owl:inverseOf :isConsumedBy ;
          rdfs:domain <http://dbpedia.org/resource/Audience> ;
          rdfs:range <http://dbpedia.org/resource/Category:Media_formats> ;
          rdfs:comment "Audience consumes Format" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#existsInAudioFormat
:existsInAudioFormat rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf <http://dbpedia.org/ontology/type> ;
                     owl:inverseOf :isAudioFormatOf ;
                     rdfs:domain :Story ;
                     rdfs:range :Audio ;
                     rdfs:comment "Story can be listened to" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#existsInTextFormat
:existsInTextFormat rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf <http://dbpedia.org/ontology/type> ;
                    owl:inverseOf :isTextFormatOf ;
                    rdfs:domain :Story ;
                    rdfs:range <http://dbpedia.org/resource/Book> ;
                    rdfs:comment "Story can be read" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#existsInVideoFormat
:existsInVideoFormat rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf <http://dbpedia.org/ontology/type> ;
                     owl:inverseOf :isVideoFormatOf ;
                     rdfs:domain :Story ;
                     rdfs:range <http://dbpedia.org/resource/Video> ;
                     rdfs:comment "Story can be watched" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#hasCharacter
:hasCharacter rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              owl:inverseOf :isCharacterOf ;
              rdfs:domain :Story ;
              rdfs:range :Character ;
              rdfs:comment "Story has Character" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#hasHero
:hasHero rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :hasCharacter ;
         owl:inverseOf :isHeroOf ;
         rdfs:domain :Story ;
         rdfs:range :MainCharacter ;
         rdfs:comment "Story has MainCharacter, which is Character" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isAudioFormatOf
:isAudioFormatOf rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :isFormatOf ;
                 rdfs:comment "AudioFormat is the representation of story" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isCharacterOf
:isCharacterOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain :Character ;
               rdfs:range :Story ;
               rdfs:comment "Character is a character of Story" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isConsumedBy
:isConsumedBy rdf:type owl:ObjectProperty ;
              rdfs:domain <http://dbpedia.org/resource/Category:Media_formats> ;
              rdfs:range <http://dbpedia.org/resource/Audience> ;
              rdfs:comment "a Media Format is consumed by an audience" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isCreatedBy
:isCreatedBy rdf:type owl:ObjectProperty ;
             rdfs:domain :Character ;
             rdfs:range <https://dbpedia.org/resource/Author> ;
             rdfs:comment """Character is created by Author
inverse property of CreatesCharacter which implies:
if Character is present in a story and story is written by Author then Character is created by Author""" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isCurrentStatus
:isCurrentStatus rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :isStatus ;
                 rdfs:domain :Story ;
                 rdfs:range :Ongoing ;
                 owl:propertyDisjointWith :isEnded ;
                 rdfs:comment "Story is not Finished" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isEnded
:isEnded rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :isStatus ;
         rdfs:domain :Story ;
         rdfs:range :Finished ;
         rdfs:comment "Story is Finished" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isFormatOf
:isFormatOf rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain <http://dbpedia.org/resource/Category:Media_formats> ;
            rdfs:range :Story ;
            rdfs:comment "Format is the representation of Story" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isGenreOfStory
:isGenreOfStory rdf:type owl:ObjectProperty ;
                rdfs:domain <http://dbpedia.org/class/yago/WikicatLiteraryGenres> ;
                rdfs:range :Story ;
                rdfs:comment "Genre is a theme of Story" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isHeroOf
:isHeroOf rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :isCharacterOf ;
          rdfs:domain :MainCharacter ;
          rdfs:range :Story ;
          rdfs:comment "A MainCharacter is a Character of Story" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isInterestedByChildStory
:isInterestedByChildStory rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :isInterestedByStory ;
                          owl:inverseOf :targetsChildAudience ;
                          rdfs:domain :ChildAudience ;
                          rdfs:range :ChildStory ;
                          rdfs:comment "ChildAudience is interested by ChildStory" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isInterestedByStory
:isInterestedByStory rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     owl:inverseOf :targetsAudience ;
                     rdfs:domain <http://dbpedia.org/resource/Audience> ;
                     rdfs:range :Story ;
                     rdfs:comment "Audience is interested by Story" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isInterestedByTeenStory
:isInterestedByTeenStory rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf :isInterestedByStory ;
                         owl:inverseOf :targetsTeenAudience ;
                         rdfs:domain :TeenAudience ;
                         rdfs:range :TeenStory ;
                         rdfs:comment "TeenAudience is interested by Teen Story" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isListenedToBy
:isListenedToBy rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :isConsumedBy ;
                owl:inverseOf :listensTo ;
                rdfs:domain :Audio ;
                rdfs:range <http://dbpedia.org/resource/Audience> ;
                owl:propertyChainAxiom ( :isAudioFormatOf
                                         :targetsAudience
                                       ) ;
                rdfs:comment "an audio format is listen to by an audience" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isReadBy
:isReadBy rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :isConsumedBy ;
          owl:inverseOf :reads ;
          rdfs:domain <http://dbpedia.org/resource/Book> ;
          rdfs:range <http://dbpedia.org/resource/Audience> ;
          owl:propertyChainAxiom ( :isTextFormatOf
                                   :targetsAudience
                                 ) ;
          rdfs:comment "a text format is read by an audience" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isStatus
:isStatus rdf:type owl:ObjectProperty ;
          rdfs:domain :Story ;
          rdfs:range :Status ;
          rdfs:comment "Story has Status" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isTextFormatOf
:isTextFormatOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :isFormatOf ;
                rdfs:comment "TextFormat is the representation of Story" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isVideoFormatOf
:isVideoFormatOf rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :isFormatOf ;
                 rdfs:comment "VideoFormat is the representation of Story" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isWatchedBy
:isWatchedBy rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :isConsumedBy ;
             owl:inverseOf :watches ;
             rdfs:domain <http://dbpedia.org/resource/Video> ;
             rdfs:range <http://dbpedia.org/resource/Audience> ;
             owl:propertyChainAxiom ( :isVideoFormatOf
                                      :targetsAudience
                                    ) ;
             rdfs:comment "a video format is watched by an audience" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#isWrittenIn
:isWrittenIn rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :Story ;
             rdfs:range :DiscourseForm ;
             rdfs:comment "Story is written under a specific form of discourse (Prose, Verse)" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#listensTo
:listensTo rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :consumes ;
           rdfs:range :Audio ;
           owl:propertyChainAxiom ( :isInterestedByStory
                                    :existsInAudioFormat
                                  ) ;
           rdfs:comment "Audience listens to audio Format" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#reads
:reads rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :consumes ;
       rdfs:range <http://dbpedia.org/resource/Book> ;
       owl:propertyChainAxiom ( :isInterestedByStory
                                :existsInTextFormat
                              ) ;
       rdfs:comment "Audience reads Text Format" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#targetsAudience
:targetsAudience rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain :Story ;
                 rdfs:range <http://dbpedia.org/resource/Audience> ;
                 rdfs:comment "Story targets an Audience" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#targetsChildAudience
:targetsChildAudience rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :targetsAudience ;
                      rdfs:domain :ChildStory ;
                      rdfs:range :ChildAudience ;
                      rdfs:comment "ChildStory targets ChildAudience" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#targetsTeenAudience
:targetsTeenAudience rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :targetsAudience ;
                     rdfs:domain :TeenStory ;
                     rdfs:range :TeenAudience ;
                     rdfs:comment "TeenStory targets TeenAudience" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#watches
:watches rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :consumes ;
         rdfs:range <http://dbpedia.org/resource/Video> ;
         owl:propertyChainAxiom ( :isInterestedByStory
                                  :existsInVideoFormat
                                ) ;
         rdfs:comment "Audience watches Video Format" .


#################################################################
#    Data properties
#################################################################

###  http://dbpedia.org/ontology/abstract
<http://dbpedia.org/ontology/abstract> rdf:type owl:DatatypeProperty ;
                                       rdfs:domain :Story ;
                                       rdfs:range rdfs:Literal ;
                                       rdfs:comment "Plot of a story" ;
                                       rdfs:label "plot" .


###  http://dbpedia.org/property/name
<http://dbpedia.org/property/name> rdf:type owl:DatatypeProperty ;
                                   rdfs:subPropertyOf owl:topDataProperty ;
                                   rdf:type owl:FunctionalProperty ;
                                   rdfs:domain :Character ,
                                               <https://dbpedia.org/resource/Author> ;
                                   rdfs:range rdfs:Literal ;
                                   rdfs:comment "Name of an entity" ;
                                   rdfs:label "Name" .


###  http://dbpedia.org/property/released
<http://dbpedia.org/property/released> rdf:type owl:DatatypeProperty ;
                                       rdfs:domain :Story ;
                                       rdfs:range xsd:integer ;
                                       rdfs:label "publicationYear" .


###  http://dbpedia.org/property/title
<http://dbpedia.org/property/title> rdf:type owl:DatatypeProperty ,
                                             owl:FunctionalProperty ;
                                    rdfs:domain :Story ;
                                    rdfs:range rdfs:Literal ;
                                    rdfs:comment "Title of a story" ;
                                    rdfs:label "title" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#CharacterDescription
:CharacterDescription rdf:type owl:DatatypeProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain :Character ;
                      rdfs:range rdfs:Literal ;
                      rdfs:comment "Description of a character, his role" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#language
:language rdf:type owl:DatatypeProperty ;
          rdfs:domain :Story ;
          rdfs:range xsd:language .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/class/yago/WikicatLiteraryGenres
<http://dbpedia.org/class/yago/WikicatLiteraryGenres> rdf:type owl:Class ;
                                                      rdfs:comment "Way of structure of a narrative"@en ;
                                                      rdfs:label "Genre"@en .


###  http://dbpedia.org/resource/Audience
<http://dbpedia.org/resource/Audience> rdf:type owl:Class ;
                                       rdfs:comment "An audience is a group of people who participate in a show or encounter a work of art, literature (in which they are called \"readers\"), theatre, music (in which they are called \"listeners\"), video games (in which they are called \"players\"), or academics in any medium. Audience members participate in different ways in different kinds of art. Some events invite overt audience participation and others allow only modest clapping and criticism and reception. (en)" ;
                                       rdfs:label "Audience" .


###  http://dbpedia.org/resource/Book
<http://dbpedia.org/resource/Book> rdf:type owl:Class ;
                                   rdfs:subClassOf <http://dbpedia.org/resource/Category:Media_formats> ;
                                   rdfs:comment "Way of sharing of a story that involves texts or pictures, rarely sounds, and is split in pages"@en ;
                                   rdfs:label "Book" .


###  http://dbpedia.org/resource/Fiction
<http://dbpedia.org/resource/Fiction> rdf:type owl:Class ;
                                      rdfs:subClassOf <http://dbpedia.org/class/yago/WikicatLiteraryGenres> ;
                                      rdfs:comment "Fiction is any creative work, chiefly any narrative work, portraying individuals, events, or places in ways that are imaginary or inconsistent with history, fact, or plausibility. In a narrow sense, \"fiction\" refers to written narratives in prose – often referring specifically to novels, novellas, and short stories. More broadly, however, fiction encompasses imaginary narratives expressed in any medium, including not just writings but also live theatrical performances, films, television programs, radio dramas, comics, role-playing games, and video games."@en ;
                                      rdfs:label "Fiction" .


###  http://dbpedia.org/resource/Poetry
<http://dbpedia.org/resource/Poetry> rdf:type owl:Class ;
                                     rdfs:subClassOf <http://dbpedia.org/class/yago/WikicatLiteraryGenres> ;
                                     rdfs:comment "Poetry (derived from the Greek poiesis, \"making\"), also called verse, is a form of literature that uses aesthetic and often rhythmic qualities of language − such as phonaesthetics, sound symbolism, and metre − to evoke meanings in addition to, or in place of, a prosaic ostensible meaning. A poem is a literary composition, written by a poet, using this principle. A Western cultural tradition (extending at least from Homer to Rilke) associates the production of poetry with inspiration – often by a Muse (either classical or contemporary)."@en ;
                                     rdfs:label "Poetry"@en .


###  http://dbpedia.org/resource/Theatre
<http://dbpedia.org/resource/Theatre> rdf:type owl:Class ;
                                      rdfs:subClassOf <http://dbpedia.org/class/yago/WikicatLiteraryGenres> ;
                                      rdfs:comment "Theatre or theater is a collaborative form of performing art that uses live performers, usually actors or actresses, to present the experience of a real or imagined event before a live audience in a specific place, often a stage. The performers may communicate this experience to the audience through combinations of gesture, speech, song, music, and dance. Elements of art, such as painted scenery and stagecraft such as lighting are used to enhance the physicality, presence and immediacy of the experience. The specific place of the performance is also named by the word \"theatre\" as derived from the Ancient Greek θέατρον (théatron, \"a place for viewing\"), itself from θεάομαι (theáomai, \"to see\", \"to watch\", \"to observe\")." ;
                                      rdfs:label "Theatre" .


###  http://dbpedia.org/resource/Video
<http://dbpedia.org/resource/Video> rdf:type owl:Class ;
                                    rdfs:subClassOf <http://dbpedia.org/resource/Category:Media_formats> ;
                                    rdfs:comment "Way of sharing of a story that involves images and often sounds, where characters are animated"@en ;
                                    rdfs:label "Video" .


###  http://dbpedia.org/resource/Category:Media_formats
<http://dbpedia.org/resource/Category:Media_formats> rdf:type owl:Class ;
                                                     rdfs:comment "Way of sharing of a story"@en ;
                                                     rdfs:label "Media Format" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Audio
:Audio rdf:type owl:Class ;
       rdfs:subClassOf <http://dbpedia.org/resource/Category:Media_formats> ;
       rdfs:comment "Way of sharing that can be listened to."@en ;
       rdfs:label "Audio" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Biography
:Biography rdf:type owl:Class ;
           rdfs:subClassOf <http://dbpedia.org/class/yago/WikicatLiteraryGenres> ;
           rdfs:comment "Narrative that was inspired about true events" ;
           rdfs:label "Biography" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Character
:Character rdf:type owl:Class ;
           rdfs:comment "Individual or entity that has a role in a narrative"@en ;
           rdfs:label "Character" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#ChildAudience
:ChildAudience rdf:type owl:Class ;
               rdfs:subClassOf <http://dbpedia.org/resource/Audience> ;
               rdfs:comment "Audience composed of person between 3 and 12 years old" ;
               rdfs:label "Child audience"@en .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#ChildStory
:ChildStory rdf:type owl:Class ;
            rdfs:subClassOf :Story ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :targetsAudience ;
                              owl:someValuesFrom :ChildAudience
                            ] ;
            rdfs:comment "Story that targets children between 3 and 12 years old"@en ;
            rdfs:label "Child Story"@en .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#DiscourseForm
:DiscourseForm rdf:type owl:Class ;
               rdfs:comment "Writting form of texts"@en ;
               rdfs:label "Discourse form"@en .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Finished
:Finished rdf:type owl:Class ;
          rdfs:subClassOf :Status ;
          owl:disjointWith :Ongoing ;
          rdfs:comment "State where the story has an end" ;
          rdfs:label "Finished"@en .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#MainCharacter
:MainCharacter rdf:type owl:Class ;
               rdfs:subClassOf :Character ;
               rdfs:comment "Protagonist of a story"@en ;
               rdfs:label "Main Character" ;
               rdfs:seeAlso "dbr:Hero" .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Ongoing
:Ongoing rdf:type owl:Class ;
         rdfs:subClassOf :Status .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Status
:Status rdf:type owl:Class ;
        rdfs:comment "Level of progress of a story"@en ;
        rdfs:label "Status"@en .


###  http://www.semanticweb.org/kevin-starlabs/ontologies/2023/1/children_stories#Story
:Story rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                    owl:onProperty <http://dbpedia.org/ontology/author> ;
                                                    owl:someValuesFrom <https://dbpedia.org/resource/Author>
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty <http://dbpedia.org/ontology/type> ;
                                                    owl:someValuesFrom <http://dbpedia.org/resource/Category:Media_formats>
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty <http://dbpedia.org/property/genre> ;
                                                    owl:someValuesFrom <http://dbpedia.org/class/yago/WikicatLiteraryGenres>
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :hasHero ;
                                                    owl:someValuesFrom :Character
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :targetsAudience ;
